<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
  <servlet>
  	<servlet-name>RestListner</servlet-name>
  	<display-name>RestListner</display-name>
  	<description>For uploading and reading Content of File</description>
  	<servlet-class>com.invoice.Controller.RestListner</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>RestListner</servlet-name>
  	<url-pattern>/RestListner</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
  <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
